{
  "name": "FrontendSite",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && vuepress dev docs",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "set NODE_OPTIONS=--openssl-legacy-provider && gh-pages -d docs/.vuepress/dist",
    "deploy:build": "set NODE_OPTIONS=--openssl-legacy-provider && npm run docs:build && gh-pages -d docs/.vuepress/dist"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "vuepress": "^1.8.2",
    "vuepress-plugin-auto-sidebar": "^2.3.2",
    "vuepress-plugin-baidu-autopush": "^1.0.1",
    "vuepress-plugin-clean-urls": "^1.1.2",
    "vuepress-plugin-seo": "^0.1.4",
    "vuepress-plugin-sitemap": "^2.3.1",
    "vuepress-plugin-smooth-scroll": "^0.0.10"
  }
}
